{
  "units": [
    {
      "keyspace": "system_auth",
      "tables": [
        "role_members",
        "roles",
        "role_permissions"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_distributed",
      "tables": [
        "view_build_status",
        "cdc_generations",
        "cdc_streams"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_schema",
      "tables": [
        "columns",
        "functions",
        "types",
        "view_virtual_columns",
        "scylla_tables",
        "triggers",
        "indexes",
        "computed_columns",
        "tables",
        "keyspaces",
        "views",
        "aggregates",
        "dropped_columns"
      ],
      "all_tables": true
    },
    {
      "keyspace": "system_traces",
      "tables": [
        "node_slow_log",
        "node_slow_log_time_idx",
        "sessions_time_idx",
        "sessions",
        "events"
      ],
      "all_tables": true
    }
  ],
  "dc": [
    "dc1",
    "dc2"
  ],
  "location": [
    "s3:backuptest-get-target"
  ],
  "retention": 3,
  "rate_limit": [
    "1000",
    "dc1:100"
  ],
  "snapshot_parallel": null,
  "upload_parallel": null,
  "continue": true
}